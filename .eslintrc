{
	"extends": "eslint:recommended",
	"sourceType": "module",
	"env": {
		"browser": true,
		"node": true
	},
	"settings": {
		"ecmascript": 6
	},
	"parserOptions": {
		"ecmaFeatures": {
			"modules": true
		}
	}
	"rules": {
		"strict": 0,
		"quotes": 0,
		"no-unused-vars": 0,
		"camelcase": 0,
		"no-underscore-dangle": 0
	}
}
